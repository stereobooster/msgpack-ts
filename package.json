{
  "name": "msgpack-ts",
  "description": "TypeScript msgpack encoder",
  "version": "0.99.1019",
  "author": "@kawanet",
  "bugs": {
    "url": "https://github.com/kawanet/msgpack-ts/issues"
  },
  "dependencies": {
    "msg-ext": "^1.0.1",
    "msg-interface": "^1.1.0",
    "msg-number": "^0.1.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "assemblyscript": "github:AssemblyScript/assemblyscript",
    "browserify": "^16.2.3",
    "mocha": "^5.2.0",
    "msgpack-test-js": "^1.0.0",
    "typescript": "^3.1.3",
    "uglify-js": "^3.3.28"
  },
  "engines": {
    "node": ">=6"
  },
  "homepage": "https://github.com/kawanet/msgpack-ts#readme",
  "keywords": [
    "messagepack",
    "msgpack",
    "typescript"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawanet/msgpack-ts.git"
  },
  "scripts": {
    "build": "make",
    "fixpack": "fixpack",
    "mocha": "./node_modules/.bin/mocha test",
    "test": "npm run mocha",
    "tsc": "./node_modules/.bin/tsc",
    "tsc-typings": "./node_modules/.bin/tsc -p typings/typings.json",
    "tsc-watch": "./node_modules/.bin/tsc --watch",
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --validate --debug",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --validate --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized"
  },
  "typings": "./typings/index.d.ts"
}
